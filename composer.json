{
  "name": "aboleon/metaframework",
  "description": "Meta Framework CMS",
  "minimum-stability": "stable",
  "license": "proprietary",
  "keywords": [
    "meta framework"
  ],
  "authors": [
    {
      "name": "Aboleon Media",
      "email": "dev@aboleon.media"
    }
  ],
  "require": {
    "php": "^8.2",
    "aboleon/laravel-inputables": "dev-main",
    "css-crush/css-crush": "^4.1",
    "intervention/image": "^2.7"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.11",
    "phpunit/phpunit": "^11.1",
    "orchestra/testbench": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Aboleon\\MetaFramework\\": "src"
    },
    "files": [
      "src/Functions/loader.php",
      "src/Mediaclass/functions.php"
    ],
    "exclude-from-classmap": [
      "publishables"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aboleon\\MetaFramework\\ServiceProvider"
      ],
      "aliases": {
        "Meta": "Aboleon\\MetaFramework\\Facades\\MetaFacade",
        "Nav": "Aboleon\\MetaFramework\\Facades\\NavFacade",
        "Mediaclass": "Aboleon\\MetaFramework\\Mediaclass\\Facades\\MediaclassFacade"
      }
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php vendor/bin/testbench package:discover --ansi"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "path",
      "url": "../package-meta-framework"
    },
    {
      "type": "path",
      "url": "../laravel-inputables"
    }
  ]
}
