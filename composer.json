{
    "name": "aboleon/metaframework",
    "description": "Meta Framework CMS",
    "minimum-stability": "stable",
    "license": "proprietary",
    "keywords": [
        "meta framework"
    ],
    "authors": [
        {
            "name": "Aboleon Media",
            "email": "dev@aboleon.media"
        }
    ],
    "require": {
        "php": "^8.1",
        "css-crush/css-crush": "^4.0",
        "intervention/image": "^2.7",
        "laravel/framework": "^10.3",
        "spatie/laravel-translatable": "^6.3",
        "symfony/mime": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "MetaFramework\\": "src"
        },
        "files": [
            "src/Functions/loader.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MetaFramework\\ServiceProvider"
            ],
            "aliases": {
                "Meta": "MetaFramework\\Facades\\MetaFacade",
                "Nav": "MetaFramework\\Facades\\NavFacade",
                "Mediclass": "MetaFramework\\Mediaclass\\Facades\\MediaclassFacade"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    }
}
